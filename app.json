{
	"expo": {
		"name": "crossmint-solana-expo-demo",
		"slug": "crossmint-solana-expo-demo",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "crossmint-solana-expo-demo",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"bundleIdentifier": "com.crossmint.expo.solana.demo",
			"supportsTablet": true
		},
		"android": {
			"package": "com.crossmint.expo.solana.demo",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "6e5f5e4e-dbdf-41f7-931e-beae6931593f"
			}
		},
		"owner": "crossmint"
	}
}
